{
    "name": "ID Grabber 2.0",
    "description": "Grab ID from Intercom",
    "version": "2.0.0",
    "manifest_version": 3,
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "permissions": [
        "tabs",
        "notifications",
        "storage"
      ],
    "content_scripts": [{
        "run_at": "document_end",
        "js": ["jquery-3.6.1.min.js","content.js"],
        "matches": ["https://app.intercom.com/*"]
    }]
}
